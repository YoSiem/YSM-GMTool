{
  "playerCheck": "player_checker('{name}')",
  "spawnMonsterAtPlayer": "spawn_monster({id}, {amount})",
  "spawnMonsterAtCoords": "spawn_monster({id}, {amount}, {x}, {y}, {layer})",
  "addItemToSelf": "add_item_self({itemId}, {amount}, {level}, {enhance}, {itmcode})",
  "giveItemToPlayer": "give_item('{target}', {itemId}, {amount}, {level}, {enhance}, {itmcode})",
  "learnSkill": "learn_skill('{target}', {skillId})",
  "learnAllJobSkills": "learn_all_job_skills('{target}')",
